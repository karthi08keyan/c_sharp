using System;  
  public class ReverseExample  
   {  
     public static void Main(string[] args)  
      {  
       int n,reverse=0,temp;
       Console.WriteLine("enter nubmber ");
       n =Convert.ToInt32(Console.ReadLine());
       while(n!=0)
       {
           temp= n%10;
           reverse=reverse*10+temp;
           n/=10;
           
       }
       Console.Write(reverse);
    }  
  }  
